
config wifi-device 'ra0'
	option type 'ralink'
	option mode '9'
	option channel 'auto'
	option txpower '100'
	option ht '20'
	option country 'US'
	option disabled '0'

config wifi-iface
	option device 'ra0'
	option network 'wlan'
	option mode 'ap'
	option encryption 'psk2'
	option key '12345678'
	option ApCliEnable '1'
	option ApCliAuthMode 'WPA2PSK'
	option ApCliEncrypType 'AES'
	option ssid 'Omega-ABCD'
	option ApCliSsid 'somewifissid'
	option ApCliPassWord 'somepassword'

config wifi-config
	option ssid 'somewifissid'
	option encryption 'WPA2PSK'
	option key 'somepassword'

